{
  "name": "open-multi-agents",
  "version": "1.0.0",
  "description": "Multi-Agent Development Platform with OpenCode + DGX Spark",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "up": "./scripts/start.sh",
    "down": "./scripts/stop.sh",
    "up:prod": "./scripts/start.sh prod",
    "dev": "dotenv -- concurrently -n opencode,backend -c blue,green \"npx opencode-ai serve\" \"tsx watch src/server.ts\"",
    "dev:backend": "tsx watch src/server.ts",
    "dev:opencode": "npx opencode-ai serve",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f",
    "build": "tsc",
    "start": "dotenv -- concurrently -n opencode,backend -c blue,green \"npx opencode-ai serve\" \"node dist/server.js\"",
    "start:backend": "node dist/server.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "test": "vitest",
    "cli": "tsx src/cli/index.ts"
  },
  "keywords": [
    "opencode",
    "ai",
    "agents",
    "typescript",
    "dgx-spark",
    "multi-agent"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@opencode-ai/sdk": "^1.1.49",
    "better-sqlite3": "^12.6.2",
    "bullmq": "^5.67.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "glob": "^11.1.0",
    "ioredis": "^5.9.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.18.0",
    "pino": "^10.3.0",
    "pino-pretty": "^13.1.3",
    "socket.io": "^4.8.1",
    "uuid": "^11.1.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.25",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.10.5",
    "@types/pg": "^8.16.0",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "concurrently": "^9.2.1",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.17.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^3.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
